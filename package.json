{
  "name": "agora-node-rtm",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Agora RTM in NodeJS",
  "repository": {
    "type": "git",
    "url": "#"
  },
  "gypfile": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --setupFiles dotenv/config --forceExit",
    "postinstall": "npm run build:ts",
    "build:addon": "node-gyp rebuild",
    "build:debug": "node-gyp rebuild --debug && mv build/Debug build/Release",
    "build:ts": "tsc",
    "sync:lib": "just sync:lib",
    "install:lib": "just sync:lib && sudo cp sdk/libagora_rtm_sdk.so /usr/lib/"
  },
  "agora": {
    "liburl": "https://download.agora.io/rtmsdk/release/Agora_RTM_SDK_for_Linux_v1_2_2.zip?_ga=2.58691050.1900749775.1582269442-543476706.1582269442"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "dependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-typescript": "7.3.3",
    "@babel/runtime": "^7.4.2",
    "@pika/pack": "0.3.5",
    "@pika/plugin-build-node": "0.3.14",
    "@pika/plugin-build-types": "0.3.14",
    "@pika/plugin-build-web": "0.3.14",
    "@pika/plugin-standard-pkg": "0.3.14",
    "@types/jest": "24.0.11",
    "@types/node": "^11.11.3",
    "bluebird": "^3.7.2",
    "dotenv": "^7.0.0",
    "download": "^7.1.0",
    "extract-zip": "^1.6.7",
    "glob": "^7.1.6",
    "jest": "24.4.0",
    "just-task": "^0.14.0",
    "nan": "^2.13.0",
    "typescript": "3.3.3333",
    "uap-lib": "0.0.22"
  },
  "devDependencies": {}
}
